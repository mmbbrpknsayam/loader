        GameId = 7018190066;
        PlaceIds = {};
        Loader = "https://api.luarmor.net/files/v3/loaders/2d9f941db1fc0f126b147f7a827a1c14.lua";
    };
}

local currentGameId = game.GameId
local loaded = false

for name, data in pairs(Projects) do
    if data.GameId == currentGameId then
        loaded = true
        print("âœ… Loader found for:", name)
        loadstring(game:HttpGet(data.Loader))()
        break
    end
end

if not loaded then
    warn("ðŸš« No loader found for GameId:", currentGameId)
end
