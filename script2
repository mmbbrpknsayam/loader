local validKeys = {
    ["Lqv5Y8Hd29MWcAz"] = true,
}

if not script_key or not validKeys[script_key] then
    game.Players.LocalPlayer:Kick("Whitelist error Invalid key. Please use the correct key.")
    return
end

local Projects = {
    [126884695634066] = "https://raw.githubusercontent.com/mmbbrpknsayam/Script/refs/heads/main/grow"; -- Grow-a-Garden
    [6516141723] = "https://raw.githubusercontent.com/mmbbrpknsayam/Script/refs/heads/main/door"; -- Door
    [2380077519] = "https://raw.githubusercontent.com/mmbbrpknsayam/Script/refs/heads/main/slap"; -- Slap-battle
    [192800] = "https://raw.githubusercontent.com/mmbbrpknsayam/Script/refs/heads/main/pizza"; -- Pizza-place
    [2380077519] = "https://raw.githubusercontent.com/mmbbrpknsayam/Script/refs/heads/main/rail"; -- dead-rails
}

local gameId = game.GameId
local loaderUrl = Projects[gameId]

if loaderUrl then
    print("✅ Loader found for GameId:", gameId)
    loadstring(game:HttpGet(loaderUrl))()
else
    warn("❌ No loader found for this GameId:", gameId)
end
