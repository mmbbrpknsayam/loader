local validKeys = {
    ["Lqv5Y8Hd29MWcAz"] = true,
}

if not script_key or not validKeys[script_key] then
    game.Players.LocalPlayer:Kick("Whitelist error Invalid key. Please use the correct key.")
    return
end

local placeId = game.PlaceId

local Projects = {
    [126884695634066] = "https://raw.githubusercontent.com/mmbbrpknsayam/Script/refs/heads/main/grow"; -- Grow-a-Garden
    [6516141723] = "https://raw.githubusercontent.com/mmbbrpknsayam/Script/refs/heads/main/door"; -- Door
}

local loaderUrl = Projects[PlaceId]
if loaderUrl then
    -- Directly load and execute the script
    loadstring(game:HttpGet(loaderUrl))()
else
    warn("No loader found for this game. PlaceId:", placeId)
end
